{
  "name": "challenge-box",
  "type": "module",
  "version": "1.0.2",
  "description": "Challenge Box Game",
  "author": "7Koston",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "clean": "nuxt cleanup",
    "clean:full": "node ./scripts/clean.mjs .output .nuxt node_modules pnpm-lock.yaml",
    "reinstall": "node ./scripts/clean.mjs .output .nuxt node_modules && pnpm install --frozen-lockfile",
    "upgrade": "pnpm clean:deps && pnpm install",
    "prepare": "nuxt prepare",
    "postprepare": "husky || true",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "postinstall": "nuxt prepare",
    "local": "nuxt dev --dotenv .env",
    "build": "nuxt build"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  },
  "engines": {
    "node": ">=24.13.1"
  },
  "packageManager": "pnpm@10.29.3",
  "dependencies": {
    "@nuxt/eslint": "1.15.1",
    "@nuxt/icon": "2.2.1",
    "@nuxt/image": "2.0.0",
    "@nuxtjs/color-mode": "4.0.0",
    "@nuxtjs/i18n": "10.2.3",
    "@vueuse/core": "^14.2.1",
    "@vueuse/nuxt": "^14.2.1",
    "es-toolkit": "^1.44.0",
    "nuxt": "^4.3.1",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.15.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^25.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.4"
  }
}
